head
  title= global.page.title
  if global.baseUrl
    base(href=global.baseUrl)
  meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
  meta(http-equiv="Content-type", content="text/html; charset=utf-8")

  meta(name='viewport', content='width=device-width,initial-scale=1')
  meta(name="keywords", content=global.page.description)
  meta(name="description", content=global.page.title)
  link(href='assets/img/favicon.ico', rel='shortcut icon', type='image/x-icon')
  link(href="assets/css/style.css", rel="stylesheet", type="text/css", media="all")
 
  meta(property='og:title', content=global.page.title)
  meta(property='og:description', content=global.page.description)
  meta(property='og:type', content='website')
  meta(property='og:url', content='APP_URL')
  meta(property='og:image', content='/assets/img/favicon.png')
  meta(property='og:site_name', content=global.page.title)
  meta(property='fb:admins', content='APP_ADMIN')

  script(src="assets/js/lib/jquery-2.0.3.min.js", type="text/javascript")
  script(src="assets/js/lib/jquery.tools.min.js", type="text/javascript")
  script(src="assets/js/lib/jquery.flexslider.js", type="text/javascript")
  script(src="assets/js/script.js", type="text/javascript")
  //if lte IE8
    script(src='assets/js/lib/html5.js')

  script.
    var _gaq=[['_setAccount','UA-31441831-4'],['_trackPageview']];
    (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
    g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
    s.parentNode.insertBefore(g,s)}(document,'script'));
