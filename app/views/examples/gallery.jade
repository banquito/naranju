extends ../_layouts/default
block nav
  header#header
    h1.logo= global.page.title
  +nav('nav', global.nav)
  +nav('nav-light', global.navejemplos)

block content
  .wrapper
    +slider(slides)
    section#content.span5.span-768-6
      h3 Carousel
      +carousel(thumbs)
      h3 Galeria a 6 columnas
      +gallery(6, gallery)
    .span7.span-768-6
      h3 Galeria a 4 columnas
      +gallery(4, gallery)
